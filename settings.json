  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "foreground": "#434343"
        }
      },
			{
        "scope": "string",
        "settings": {
          "foreground": "#ffe772",
					"fontStyle": "italic"
        }
      },
			{
        "scope": [
					"storage.type",
					"keyword.control.export.ts",
					"keyword.control.conditional.ts",
					"keyword.control.loop.ts",
					"keyword.control.else.ts",
					"keyword.operator.new.ts"
				],
        "settings": {
          "foreground": "#00ffb7",
        }
      },
			{
        "scope": [
					"support.type",
					"keyword.control.import.ts",
					"keyword.control.from.ts",
					"keyword.control.as.ts",
				],
        "settings": {
          "foreground": "#40a6ff",
        }
      },
			{
        "scope": [
					"storage.modifier.async.ts",
					"keyword.control.flow.ts"
				],
        "settings": {
          "foreground": "#00ffb7",
					"fontStyle": "italic"
        }
      },
			{
        "scope": [
					"storage.modifier.ts",
					"support.type.property-name.json"
				],
        "settings": {
          "foreground": "#00e5ff",
        }
      },
			{
        "scope": [
					"entity.name.type",
					"support.class",
				],
        "settings": {
          "foreground": "#ffb625",
					"fontStyle": "bold"
        }
      },
			{
        "scope": [
					"entity.name.tag.html"
				],
        "settings": {
          "foreground": "#00e5ff",
        }
      },
			{
        "scope": [
					"variable",
					"support.variable",
					"entity.other.attribute-name.html"
				],
        "settings": {
          "foreground": "#f8f8f8",
        }
      },
			{
        "scope": [
					"variable.parameter",
				],
        "settings": {
          "foreground": "#f8f8f8",
					"fontStyle": "italic underline"
        }
      },
			{
        "scope": [
					"variable.other.constant.property.ts"
				],
        "settings": {
          "foreground": "#ffffff",
					"fontStyle": "bold"
        }
      },
			{
        "scope": [
					"variable.other.constant.object.ts"
				],
        "settings": {
          "foreground": "#ffffff",
					"fontStyle": "italic"
        }
      },
			{
        "scope": [
					"variable.language", //this, super
					"constant.language.import-export-all.ts"
				],
        "settings": {
          "foreground": "#8f8f8f",
        }
      },
			{
        "scope": [
					"constant.language.null.ts",
				],
        "settings": {
          "foreground": "#747474",
					"fontStyle": "underline"
        }
      },
			{
        "scope": "constant.language.undefined.ts",
        "settings": {
          "foreground": "#747474",
					"fontStyle": "strikethrough"
        }
      },
			{
        "scope": "constant.language.boolean.false.ts",
        "settings": {
          "foreground": "#ff4d4d",
        }
      },
			{
        "scope": "constant.language.boolean.true.ts",
        "settings": {
          "foreground": "#00ff26",
        }
      },
			{
        "scope": "constant.numeric",
        "settings": {
          "foreground": "#ff40e2",
        }
      },
			{
        "scope": "entity.name.type.enum",
        "settings": {
          "foreground": "#00ff11",
        }
      },
			{
        "scope": "variable.other.enummember",
        "settings": {
          "foreground": "#94ff9b",
					"fontStyle": ""
        }
      },
			{
        "scope": [
					"keyword.operator",
					"punctuation.decorator.ts"
				],
        "settings": {
          "foreground": "#ffffff",
        }
      },
			{
        "scope": [
					"keyword.operator.optional.ts",
					"keyword.operator.relational.ts",
					"keyword.operator.comparison.ts",
					"keyword.operator.logical.ts"
				],
        "settings": {
          "foreground": "#b7ff00",
        }
      },
			{
        "scope": [
					"punctuation.terminator.statement.ts",
					"keyword.operator.type.annotation.ts",
					"keyword.operator.assignment.ts"
				],
        "settings": {
          "foreground": "#bcbcbc",
        }
      },
			{
        "scope": [
					"entity.name.function.member",
					"support.function.ts",
					"entity.name.function.ts"
				],
        "settings": {
          "foreground": "#ffde4a",
        }
      },
			{
        "scope": [
					"entity.other.ng-binding-name.event.html"
				],
        "settings": {
          "foreground": "#93ff3a",
					"fontStyle": "bold"
        }
      },
    ]
  }
}
